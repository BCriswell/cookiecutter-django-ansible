{% raw %}[program:{{project_name}}]
command={{ virtualenv_dir }}/bin/uwsgi --ini /etc/uwsgi/vassals/{{project_name}}.ini
autostart=true
autorestart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/uwsgi.log
stopwaitsecs={% if is_production %}60{% else %}0{% endif %}

stopsignal=HUP
user={{ django_user }}
group=www-data{% endraw %}
